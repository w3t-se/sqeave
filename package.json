{
  "name": "@w3t-ab/sqeave",
  "version": "0.6.0",
  "description": "A squint-cljs based solid-js framework for building SPAs borrowing ideas from Fulcro.",
  "author": "W3T AB",
  "license": "EPL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3t_ab/sqeave.git"
  },
  "type": "module",
  "module": "index.mjs",
  "files": [
      "dist",
    "index.mjs",
    "comp.js",
    "utils.js",
    "transact.js",
    "normad.js"
  ],
  "scripts": {
    "build": "squint compile src/main/**/**/*.cljs --extension mjs && cp src/main/w3t-ab/sqeave/*.cljc dist/w3t-ab/sqeave/",
    "test": "pnpm build && pnpm test:build && vitest --run",
    "test:build": "squint compile src/test/**/**/*.cljs --extension mjs  && cp -r src/test/w3t-ab/sqeave/data dist/src/test/w3t-ab/sqeave"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "lodash-es": "^4.17.22",
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.9.1",
    "jsdom": "^27.3.0",
    "squint-cljs": "^0.9.182",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cljs",
    "clojurescript",
    "squint-cljs"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs"
    },
    "./solid": {
      "import": "./index.mjs"
    },
    "./comp": {
      "import": "./comp.js"
    },
    "./normad": {
      "import": "./normad.js"
    },
    "./utils": {
      "import": "./utils.js"
    },
    "./transact": {
      "import": "./transact.js"
    }
  },
  "bugs": {
    "url": "https://github.com/w3t_ab/sqeave/issues"
  },
  "homepage": "https://github.com/w3t_ab/sqeave#readme"
}
